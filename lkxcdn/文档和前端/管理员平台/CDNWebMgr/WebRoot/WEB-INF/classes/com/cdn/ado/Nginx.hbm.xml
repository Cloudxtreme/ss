<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.cdn.ado">
    <class name="Nginx"   table="user_nginx">
       <id name="id"   column="id" type="java.lang.Integer">
         <generator class="identity"></generator>
       </id>    
       <property name="user" column="user" type="java.lang.String"></property>
       <property name="ip" column="ip" type="java.lang.String"></property>
       <property name="port" column="port" type="java.lang.Integer"></property>
       <property name="status" column="status" type="java.lang.String"></property>
      
    </class>
    
	
</hibernate-mapping>